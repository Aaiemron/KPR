	<include content="textbox_parametres">																										<description>textbox description</description>
		<param name="id"/>
		<param name="label" value="$VAR[description_parametre]"/>
	</include>
	<control type="image">           																											<description>ombre cadre gauche</description>
		<top>63</top>
		<left>460</left>
		<width>20</width>
		<height>1000</height>
		<aspectratio>stretch</aspectratio>
		<texture>fenetres/ombre_droite.png</texture>	
	</control>
	</control>	
