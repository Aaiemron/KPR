	</include>
	<include content="boutons_hauts_fenetre">
		<param name="id" value="21"/>
		<param name="ondown" value="570"/>	
	</include>
	<include content="boutons_hauts_fenetre">
		<param name="id" value="23"/>
		<param name="label" value="Paramètres"/>
		<param name="ondown" value="570"/>
		<param name="onclick" value="ActivateWindow(systemsettings,logging)"/>	
	</include>	
	</control>	
