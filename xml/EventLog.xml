		<bottom>0</bottom>
		<right>10</right>
		<left>460</left>
		<onup>8000</onup>
		<onright>60</onright>
		<onleft>220</onleft>		
		<onback>8000</onback>
		<itemgap>0</itemgap>
		<pagecontrol>60</pagecontrol>
		<scrolltime tween="Linear" easing="in">250</scrolltime>	
	<itemlayout height="76">	
