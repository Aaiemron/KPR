		<left>463</left>
		<ondown>570</ondown>
		<onleft>220</onleft>
		<itemgap>3</itemgap>
		<orientation>horizontal</orientation>
		<scrolltime tween="Linear" easing="in">250</scrolltime>		
	<include content="boutons_hauts_fenetre">
		<param name="id" value="4"/>
		<param name="label" value="Ordre $INFO[Container(570).SortOrder]"/>	
		<param name="ondown" value="570"/>
	</include>
	<include content="boutons_hauts_fenetre">
		<param name="id" value="20"/>
		<param name="label" value="Effacer les données"/>
