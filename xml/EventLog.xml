		<left>48</left>
		<width>364</width>
		<height>60</height>
		<textoffsetx>20</textoffsetx>
		<align>left</align>
		<aligny>center</aligny>
		<textoffsetx>5</textoffsetx>
		<font>bouton</font>
		<textcolor>$VAR[panneau_fenetre]</textcolor>
		<focusedcolor>$VAR[focus_texte_bouton_fenetre]</focusedcolor>
		<texturenofocus colordiffuse="$VAR[texte_bouton_fenetre]">texture.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[focus_bouton_fenetre]">texture.png</texturefocus>	
		<textureradioonfocus colordiffuse="$VAR[focus_texte_bouton_fenetre]">boutons/radiobutton_focus.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[panneau_fenetre]">boutons/radiobutton_focus.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="$VAR[focus_texte_bouton_fenetre]">boutons/radiobutton_no_focus.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="$VAR[panneau_fenetre]">boutons/radiobutton_no_focus.png</textureradiooffnofocus>		
