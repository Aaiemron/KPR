		<selected>String.contains(Control.GetLabel(22),*) + !String.contains(Control.GetLabel(21),err)</selected>
		<onright>8000</onright>
		<onclick>Sendclick(22)</onclick>
		<ondown condition="player.hasvideo">120</ondown>				
	</control>
	<control type="radiobutton" id="22"> 																										<description>bouton cache pour recuperer le niveau du rapport</description> 
		<left>2000</left>
	</control>
	</control>	
	<control type="panel" id="570">																												<description>liste du rapport</description>
