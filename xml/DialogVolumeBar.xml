<?xml version="1.0" encoding="UTF-8"?>
<window>	
	<backgroundcolor>00X000000</backgroundcolor>
	<zorder>10</zorder>
<controls>	
	<control type="image">																														<description>fond titre OSD</description>
		<centerleft>50%</centerleft>
		<top>5</top>
		<width>140</width>
		<height>100</height>
		<aspectratio>stretch</aspectratio>
		<texture colordiffuse="$VAR[couleur_OSD]">commun/texture.png</texture>
		<visible>window.isvisible(fullscreenvideo) + !window.isvisible(VideoOSD.xml) + !player.Paused + !Player.Forwarding + !Player.Rewinding + !player.showinfo</visible>
	</control>
	<control type="group">
		<centerleft>50%</centerleft>
		<top>15</top>
		<width>80</width>
		<height>80</height>
	<control type="progress" id="1111">
		<info>Player.Volume</info>
		<visible>false</visible>
	</control>	
	<control type="image">
		<texture colordiffuse="ff808080">boutons/volume/p100.png</texture>
	</control>
	<control type="image">
		<texture>$INFO[Control.GetLabel(1111),boutons/volume/p,.png]</texture>
		<visible>!player.muted</visible>
	</control>	
	<control type="image">
		<left>25</left>
		<top>25</top>
		<width>30</width>
		<height>30</height>
		<texture>boutons/volume/volume.png</texture>
		<visible>!player.muted + !player.passthrough</visible>
	</control>
	<control type="image">
		<left>25</left>
		<top>25</top>
		<width>30</width>
		<height>30</height>
		<texture colordiffuse="ffc0392b">boutons/volume/volume.png</texture>
		<visible>player.muted + !player.passthrough</visible>
	</control>
	<control type="image">
		<left>25</left>
		<top>25</top>
		<width>30</width>
		<height>30</height>
		<texture colordiffuse="ffc0392b">boutons/volume/passthrough.png</texture>
		<visible>player.passthrough</visible>
	</control>
	</control>	
</controls>
</window>